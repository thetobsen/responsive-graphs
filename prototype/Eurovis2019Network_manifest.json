{
	"identifier": "id",
	"node_attributes": [
		{
			"key": "screen_name",
			"name": "Twitter Name",
			"short_name": "Name",
			"type": "string",
			"filter": "input",
			"flags": "dod|primary|label"
		},
		{
			"key": "followers_count",
			"name": "Followers overall",
			"short_name": "Followers",
			"type": "int",
			"filter": "range",
			"flags": "layout|dod|label",
			"additional": {
				"effective_range": [0, 50000]
			}
		},
		{
			"key": "query_tweet_count",
			"name": "Number of Tweets using #Eurovis2019",
			"short_name": "Tweets #EuroVis2019",
			"type": "int",
			"filter": "range",
			"flags": "layout|dod|label"
		},
		{
			"key": "friends_count",
			"name": "Follows",
			"short_name": "Follows",
			"type": "int",
			"filter": "range",
			"flags": "layout|dod|label",
			"additional": {
				"effective_range": [0, 5000]
			}
		},
		{
			"key": "statuses_count",
			"name": "Number of Tweets",
			"short_name": "Tweets",
			"type": "int",
			"filter": "range",
			"flags": "layout|dod|label",
			"description": "Number of all Tweets including Retweets"
		},
		{
			"key": "listed_count",
			"name": "Number of lists added to",
			"short_name": "Lists",
			"type": "int",
			"filter": "range",
			"flags": "layout|dod|label"
		},
		{
			"key": "favourites_count",
			"name": "Number of liked tweets",
			"short_name": "Liked tweets",
			"type": "int",
			"filter": "range",
			"flags": "layout|dod|label"
		},
		{
			"key": "count_followers_in_query",
			"name": "Followers on Eurovis2019",
			"short_name": "Followers on EuroVis",
			"type": "int",
			"filter": "range",
			"flags": "layout|dod|label"
		},
		{
			"key": "id",
			"name": "Profile ID",
			"short_name": "Profile ID",
			"type": "int",
			"filter": "input",
			"flags": "label"
		},
		{
			"key": "profile_image_url",
			"name": "Profile image",
			"short_name": "Avatar",
			"type": "string",
			"filter": null,
			"flags": "img|dod",
			"additional": {
				"className": "profile-img"
			}
		},
		{
			"key": "influential",
			"name": "Influential",
			"short_name": "Influential",
			"type": "boolean",
			"filter": "checkbox",
			"flags": "dod|label|layout",
			"description": "Whether >8 followers on EuroVis19 or so",
			"additional": {
				"options": [
					{ "value": true, "label": "Influential" },
					{ "value": false, "label": "Non-influential" }
				]
			}
		},
		{
			"key": "original",
			"name": "Original",
			"short_name": "Original",
			"type": "boolean",
			"filter": "checkbox",
			"flags": "dod|label|layout",
			"description": "Whether within the 45 accounts tweeting #Eurovis2019",
			"additional": {
				"options": [
					{ "value": true, "label": "Original" },
					{ "value": false, "label": "Non-original" }
				]
			}
		},
		{
			"key": "utc_offset",
			"name": "UTC Offset",
			"short_name": "UTC Offset",
			"type": "null",
			"filter": null,
			"flags": ""
		}
	],
	"pre_process": {
		"filter": {
			"edge": [
				{
					"type": "undirected",
					"key": "id"
				}
			]
		}
	},
	"layout": {
		"default": {
			"xAxis": "followers_count",
			"yAxis": "count_followers_in_query",
			"sizeAttr": "followers_count",
			"colorAttr": "followers_count"
		}
	}
}